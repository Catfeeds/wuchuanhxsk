}

.li_title {
  width: 100%;
  padding-top: 10rpx;
  height: 30rpx;
  max-width: 85%;
  margin: 0;
  line-height: 26rpx;
  font-size: 32rpx;
  font-weight: normal;
  color: #333;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}

.title {
  width: auto;
  float: left;
}

.rzpic {
